<UserControl x:Class="UCP.Views.TabViews.Bugfixes"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:UCP.Views.TabViews"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800"
             >
    <ScrollViewer>
        <StackPanel>
            <TreeViewItem IsExpanded="false" VerticalAlignment="Stretch" HorizontalAlignment="Stretch">
                <TreeViewItem.Header>
                    <CheckBox Content="{DynamicResource u_fireballistafix}"/>
                </TreeViewItem.Header>
                <TextBlock Text="{DynamicResource Deutsch}"/>
            </TreeViewItem>
        </StackPanel>
    </ScrollViewer>
</UserControl>
